{
  "manifest_version": 3,
  "name": "Youtube Downloader do Paulinho",
  "version": "1.0",
  "description": "Botão verde estilo hacker para baixar vídeos",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": [
    "*://*.youtube.com/*", 
    "http://localhost:5000/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}